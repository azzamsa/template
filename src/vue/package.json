{
  "name": "mold",
  "version": "1.1.0",
  "packageManager": "pnpm@7.9.0",
  "scripts": {
    "dev": "vite --port 4173",
    "preview": "vite preview --port 4173",
    "test:e2e": "start-server-and-test preview http://localhost:4173/ 'cypress run --e2e'",
    "test:unit": "vitest --run",
    "test": "run-s test:*",
    "fmt:rustywind": "rustywind --write .",
    "fmt:eslint": "eslint . --fix",
    "fmt": "run-s fmt:*",
    "lint:rustywind": "rustywind --check-formatted .",
    "lint:vue": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint:eslint": "eslint .",
    "lint": "run-s lint:*",
    "build-only": "vite build",
    "build": "run-p lint:vue build-only"
  },
  "dependencies": {
    "@vueuse/core": "^9.4.0",
    "@vueuse/head": "^0.9.8",
    "daisyui": "^2.38.0",
    "pinia": "^2.0.23",
    "vue": "^3.2.41",
    "vue-i18n": "9.2.2",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.29.3",
    "@iconify/json": "^2.1.132",
    "@intlify/vite-plugin-vue-i18n": "^6.0.3",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^18.11.9",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.2.1",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.13",
    "cypress": "^10.11.0",
    "eslint": "^8.26.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.7.0",
    "husky": "^8.0.1",
    "jsdom": "^20.0.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.18",
    "rustywind": "^0.15.1",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.2.1",
    "typescript": "~4.8.4",
    "unplugin-auto-import": "^0.11.4",
    "unplugin-icons": "^0.14.13",
    "unplugin-vue-components": "^0.22.9",
    "vite": "^3.2.2",
    "vite-plugin-pages": "^0.27.1",
    "vite-plugin-vue-layouts": "^0.7.0",
    "vitest": "^0.24.5",
    "vue-tsc": "^1.0.9"
  }
}

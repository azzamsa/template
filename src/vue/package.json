{
  "name": "mold",
  "version": "1.1.0",
  "packageManager": "pnpm@7.9.0",
  "scripts": {
    "dev": "vite --port 4173",
    "preview": "vite preview --port 4173",
    "test-e2e": "start-server-and-test preview http://localhost:4173/ 'cypress run --e2e'",
    "test-unit": "vitest --run",
    "test": "run-s test-*",
    "fmt:rustywind": "rustywind --write .",
    "fmt:eslint": "eslint . --fix",
    "fmt": "run-s fmt:*",
    "lint:rustywind": "rustywind --check-formatted .",
    "lint:vue": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint:eslint": "eslint .",
    "lint": "run-s lint:*",
    "build-only": "vite build",
    "build": "run-p lint:vue build-only"
  },
  "dependencies": {
    "@vueuse/core": "^9.1.1",
    "@vueuse/head": "^0.7.9",
    "daisyui": "^2.24.0",
    "pinia": "^2.0.21",
    "vue": "^3.2.37",
    "vue-i18n": "9.2.2",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.26.2",
    "@iconify/json": "^2.1.100",
    "@intlify/vite-plugin-vue-i18n": "^5.0.1",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^18.7.14",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.0.2",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.8",
    "cypress": "^10.6.0",
    "eslint": "^8.23.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.4.0",
    "husky": "^8.0.1",
    "jsdom": "^20.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.16",
    "rustywind": "^0.15.0",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.1.8",
    "typescript": "~4.7.4",
    "unplugin-auto-import": "^0.11.2",
    "unplugin-icons": "^0.14.8",
    "unplugin-vue-components": "^0.22.4",
    "vite": "^3.0.9",
    "vite-plugin-pages": "^0.25.0",
    "vite-plugin-vue-layouts": "^0.7.0",
    "vitest": "^0.20.3",
    "vue-tsc": "^0.39.5"
  }
}
